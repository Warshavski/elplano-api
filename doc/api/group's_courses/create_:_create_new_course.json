    {
      "scope": [
        "course"
      ],
      "name": "active",
      "description": "Course actuality flag(not archived course) default: true"
    },
    {
      "scope": [
        "course"
      ],
      "name": "lecturer_ids",
      "description": "Lecturers of the course."
    }
  ],
